image/*; view_attachment %s -a Preview.app
image/*; (anytopnm %s | pnmscale -xysize 80 46 | ppmtopgm | pgmtopbm | pbmtoascii -1x2 ) 2>&1 ; copiousoutput
video/*; view_attachment %s -a Movist.app
text/enriched; view_attachment %s -a TextEdit.app
text/html; view_attachment %s -a "Google Chrome.app"
text/html; elinks -default-mime-type text/html %s; needsterminal; copiousoutput;
application/x-gunzip; gzcat; copiousoutput
application/x-tar-gz; gunzip -c %s | tar -tf -t ; copiousoutput
application/postscript; open -f -a Preview.app
application/pdf; view_attachment %s -a Skim.app
image/pdf; view_attachment %s -a Preview.app
# multipart/alternative; cat; copiousoutput
